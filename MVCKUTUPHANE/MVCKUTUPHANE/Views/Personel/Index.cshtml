
@using MVCKUTUPHANE.Models.Entity
@model List<TBL_PERSONELLER>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Layout.cshtml";
}


<table class="table table-bordered">
    <tr>
        <th>PERSONEL</th>
        <th>İŞLEMLER</th>
    </tr>

    @foreach (var x in Model)
    {
        <tr>
            <td>@x.PERSONEL</td>
            <td>
                <a href="/Personel/Sil/@x.ID" class="btn btn-danger">SİL</a>
                <a href="/Personel/Getir/@x.ID" class="btn btn-warning">GÜNCELLE</a>
            </td>
        </tr>
    }


</table>
<a href="/Personel/YeniPersonel" class="btn btn-primary">YENİ PERSONEL EKLE</a>

