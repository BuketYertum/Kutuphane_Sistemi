
@model MVCKUTUPHANE.Models.Entity.TBL_PERSONELLER

@{
    ViewBag.Title = "YeniPersonel";
    Layout = "~/Views/Shared/Layout.cshtml";
}


<form method="post" class="form-group">

    <label style="margin-top:15px">PERSONEL</label>
    @Html.TextBoxFor(p => p.PERSONEL, new { @class = "form-control" })
    @Html.ValidationMessageFor(p => p.PERSONEL, "", new { @style = "color:red" })

    <br/>
    <button style="margin-top:15px" class="btn btn-success">Personel Ekle</button>
</form>
